{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "warn"
    }
}
